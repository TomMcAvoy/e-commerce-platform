{
  "name": "whitestartups-shopping-online",
  "version": "1.0.0",
  "description": "Multi-vendor e-commerce platform with dropshipping integration",
  "main": "dist/index.js",
  "scripts": {
    "setup": "npm install && cd frontend && npm install && cd .. && npm run build",
    "dev:all": "echo 'ðŸš€  Starting all development servers...' && concurrently \"npm run dev:server\" \"npm run dev:frontend\"",
    "dev:server": "echo 'ðŸ”¥  Starting backend server on port 3000...' && nodemon src/index.ts",
    "dev:frontend": "echo 'ðŸŽ¨  Starting frontend server on port 3001...' && cd frontend && next dev -p 3001",
    "stop": "pkill -f \"node.*3000\" && pkill -f \"next.*3002\" || true",
    "kill": "lsof -ti:3000,3001 | xargs kill -9 || true",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest --config jest.config.js",
    "test:comprehensive": "chmod +x run-all-tests.sh && ./run-all-tests.sh",
    "test:api": "curl -f http://localhost:3000/health && curl -f http://localhost:3000/api/status",
    "test:e2e": "node tests/e2e/run-tests.js",
    "seed": "ts-node src/scripts/seed.ts",
    "cleanup": "echo 'ðŸ§¹  Wiping database completely...' && ts-node src/seeders/cleanup.ts",
    "seed:real": "echo 'ðŸŒ±  Seeding database with REAL data...' && ts-node src/seeders/seedReal.ts",
    "reset:real": "echo 'ðŸ”„  Resetting database with REAL data...' && npm run cleanup && npm run seed:real",
    "test:unit": "jest --testPathPattern=src/",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:health": "./run-all-tests.sh",
    "test:backend": "jest --testPathPattern=src/",
    "test:frontend": "cd frontend && npm test",
    "test:all": "npm run test:backend && npm run test:frontend"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.8.7",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "redis": "^4.6.7",
    "stripe": "^12.17.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.96",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.19.9",
    "@types/supertest": "^6.0.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.1.10",
    "puppeteer": "^24.15.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "e-commerce",
    "dropshipping",
    "typescript",
    "express",
    "nextjs"
  ],
  "author": "Thomas McAvoy",
  "license": "MIT"
}
