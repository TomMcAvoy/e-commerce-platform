{
  "jest.jestCommandLine": "npm test --",
  "jest.autoRun": "off",
  "jest.showCoverageOnLoad": false,
  "jest.debugMode": true,
  "typescript.preferences.includePackageJsonAutoImports": "on",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.exclude": {
    "**/coverage": true,
    "**/*.backup.*": true,
    "**/node_modules": true
  },
  "search.exclude": {
    "**/coverage": true,
    "**/*.backup.*": true
  },
  "notebook.lineNumbers": "on"
}
